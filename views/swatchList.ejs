<!DOCTYPE html>
<html>
<head>
    <%- include( 'partials/head', {}) %>
</head>
<body>
<li class="container">
    <header>
        <%- include('partials/headers', {} ) %>
    </header>
    <main>
        <ul class="list-group">
            <%
            let cnt=1;
            swatches.forEach(function(swatch) {
                let target = "http://www.mskatty.com/swatches/" + swatch.name + "_thumb.jpg";
            %>
                <li class="list-group-item"><%= cnt%>.   <%= swatch.desc %> <img src=<%= target%>
                     width="50px" height="50px" />
                </li>

            <% cnt++;  }); %>
        </ul>


    </main>
    <footer>
        <%- include('partials/footer') %>
    </footer>
</div>
</body>
</html>